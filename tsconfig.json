{
	"compilerOptions": {
		"target": "es2021",
		"lib": ["es2021"],
		"jsx": "react-jsx",
		"module": "es2022",
		"moduleResolution": "node",
		"types": ["@cloudflare/workers-types/2023-07-01", "vite-plugin-compile-time/client"],
		"resolveJsonModule": true,
		"checkJs": false,
		"noEmit": true,
		"isolatedModules": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	}
}
